  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "paths": {
      "@/*": [
        "./*"
      ],
    }
  },
  "extends": "expo/tsconfig.base"
}
